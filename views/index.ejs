<!doctype html> 
<html lang="en">
    <head>
        <%- include('partials/head')%>
    </head>

    <body class="c-app">

        <div class="c-sidebar c-sidebar-dark c-sidebar-show c-sidebar-fixed">
            <%- include('partials/sidebar')%>
        </div>

        <div class="c-wrapper">

            <%- include('partials/alert')%>

            <header>
                <div class="container-fluid">
                    <nav aria-label="breadcrumb">
                    <ol class="breadcrumb my-0 ms-2">
                        <li class="breadcrumb-item">
                        <!-- if breadcrumb is single--><span>Home</span>
                        </li>
                        <li class="breadcrumb-item active"><span>Dashboard</span></li>
                    </ol>
                    </nav>
                </div>
            </header>

            <div class="c-body">

                <main class="c-main">
        
                    <div class="container-fluid">

                        <div class="card">
                            <div class="card-body">

                                <canvas id="soil_chart"></canvas>
                                <p class="card-text">Representing state of soil</p>

                            </div>
                        </div>

                        <div class="card">
                            <div class="card-body">

                                <canvas id="light_chart"></canvas>

                                <button type="button" onclick="alertTrigger()" class="btn btn-primary"><span class="cil-contrast btn-icon mr-2"></span> Primary</button>

                                <p class="card-text">Representing state of light</p>

                            </div>
                        </div>

                    </div>
                </main>
            </div>

            <footer class="c-footer">
                <%- include('partials/footer')%>
            </footer>

        </div>

        <!-- Optional JavaScript -->
        <!-- Popper.js first, then CoreUI JS
        <script src="https://unpkg.com/@popperjs/core@2"></script>
        <script src="https://unpkg.com/@coreui/coreui/dist/js/coreui.min.js"></script>
        -->

        <!--
        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/3.2.1/chart.min.js"></script>
        <script src="sensor_data.js"></script>
        <script src="index.js"></script>
        -->
        <script src="/socket.io/socket.io.js"></script>
        <script src="test_index.js"></script>
    </body>
</html>